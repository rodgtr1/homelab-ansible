entryPoints:
  websecure:
    address: ":443"

providers:
  docker:
    exposedByDefault: false
  file:
    filename: /dynamic.yml

api:
  dashboard: true

certificatesResolvers:
  dns:
    acme:
      email: info@{{ base_domain }}
      storage: /acme.json
      dnsChallenge:
        provider: cloudflare
